# Copy to .env and fill as needed (no real secrets committed)
# Google OAuth client JSON path (download from Google Cloud Console)
GOOGLE_OAUTH_CLIENT_JSON=./.secrets/google_client.json
# Where to store the local OAuth token after first sign-in
GOOGLE_OAUTH_TOKEN_JSON=./.secrets/google_token.json

# Comma-separated list of Google Sheets URLs to extract from
GOOGLE_SHEETS_URLS=
